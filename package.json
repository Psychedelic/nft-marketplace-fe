  "scripts": {
    "start": ".scripts/start.sh",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier '**/*.{js,ts,tsx}' --write",
    "services:init": "git submodule update --init --recursive && cd ./jelly && make init",
    "services:local-replica": "cd ./jelly && dfx start --clean",
    "services:start": "cd ./jelly && make local",
    "services:reset": "cd ./jelly && make clean-dfx && cd ../nft-marketplace && dfx stop && npm run reset",
    "services:start-v1": ".scripts/deploy-services-along-v2.sh",
    "mock:generate-tokens": ".scripts/generate-mocked-tokens-v2.sh",
    "mock:generate-tokens-v1": ".scripts/generate-mocked-tokens.sh",
    "kyasshu:redis": "redis-server /usr/local/etc/redis.conf",
    "kyasshu:redis-flush-all": "redis-cli flushall",
    "kyasshu:redis-shutdown": "redis-cli shutdown",
    "kyasshu:start": ".scripts/kyasshu/start.sh",
    "kyasshu:cache": ".scripts/kyasshu/cache-tokens.sh",
    "kyasshu:reset": "pkill java"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
